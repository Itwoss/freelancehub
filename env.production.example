# Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# Database
DATABASE_URL="your_production_database_url_here"
MONGODB_URI="your_production_mongodb_uri_here"

# NextAuth
NEXTAUTH_URL="https://your-domain.com"
NEXTAUTH_SECRET="your_production_nextauth_secret_here"

# Razorpay (Production Keys)
NEXT_PUBLIC_RAZORPAY_KEY_ID="your_production_razorpay_key_id"
RAZORPAY_KEY_SECRET="your_production_razorpay_secret"
RAZORPAY_WEBHOOK_SECRET="your_production_webhook_secret"

# Email Configuration
EMAIL_USER="your_production_email@domain.com"
EMAIL_PASS="your_production_email_password"
EMAIL_FROM="noreply@your-domain.com"

# Admin Configuration
ADMIN_EMAIL="admin@your-domain.com"
NEXT_PUBLIC_BASE_URL="https://your-domain.com"

# Environment
NODE_ENV="production"
